}
#no_shadow:hover {
	background-color: transparent;
}
table {
	border-collapse: collapse;
	width: 800px;
//	margin: auto;
}

.system_table {
	border-collapse: collapse;
	width: 96%;
//	margin: auto;
}

#paper {
	box-shadow: 2px 1px 10px 2px rgba(0, 0, 0, 0.2), 0 6px 20px 0 rgba(0, 0, 0, 0.2);
	width: 97%;
	margin: auto;
	padding: 5px 0px 5px 5px;
	
}
th {
	height: 30px;
	text-align: left;
	vertical-align: middle;
	padding: 5px;
	color: #1b7ce4;
	font-size: 12pt;
	border-bottom: 3px solid #1b7ce4;
}

td {
	font-size: 12pt;
	color: #000000;
	vertical-align: middle;
	padding: 5px;
}

div tr:nth-child(even)
{
	background-color: #e6e6e6;
}
div tr:first-child:hover {
	background-color: #ffffff;
}
div tr:hover {
	background-color: #bad7f7;
}

#nav_link {
    text-decoration: none;
    color: white;
    font-size: 18px;
    padding: 6px;
    display:inline-block;
}

ul {
    list-style-type: none;
    margin: 0;
    padding: 0;
    overflow: hidden;
    background-color: #333;
	box-shadow: 0 4px 8px 0 rgba(0, 0, 0, 0.2), 0 6px 20px 0 rgba(0, 0, 0, 0.19);
	
}

a {
	text-decoration: none;
	color: inherit;
}


li {
    float: left;
}

li a, .dropbtn {
	display: inline-block;
	color: white;
	text-align:left;
	padding: 14px 7px;
	text-decoration: none;
}

li a:hover, .dropdown:hover .dropbtn {
    background-color: #ff1a1a;
}

.dropdown {
    display: inline-block;
}

.dropdown:hover {
	background-color: #ff1a1a;
}

.dropdown-content {
    display: none;
    position: absolute;
    background-color: #000000;
    min-width: 160px;
    
}

.dropdown-content a {
    color: black;
    padding: 12px 16px;
    text-decoration: none;
    display: block;
}

.dropdown-content a:hover {background-color: #333;}

.dropdown:hover .dropdown-content {
    display: block;
}

li a {
    display: inline-block;
    color: white;
    text-align: center;
    padding: 12px 14px;
    text-decoration: none;
}

.tooltip {
	position: relative;
	display: inline-block;
}

.tooltip .tooltiptext {
	visibility: hidden;
	width: 200px;
	background-color: black;
	color: #fff;
	text-align: left;
	padding: 5px;
	border-radius: 3px;
	position: absolute;
	z-index: 1;
	vertical-align: middle;
}

.tooltip:hover .tooltiptext {
	visibility: visible;
}

li a:hover {
    background-color: #ff1a1a;
}
ul li {display: inline-block;}
ul li:hover {background: #ff1a1a;}
ul li:hover ul {display: block;}
ul li ul {
  position: absolute;
  width: auto;
  display: none;
}
ul li ul li { 
  background: inherit; 
  display: block; 
}
ul li ul li a {display:block !important;} 
ul li ul li:hover {background: #666;}

